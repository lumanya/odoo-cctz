<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="seq_loan_request" model="ir.sequence">
            <field name="name">Asset Number</field>
            <field name="code">asset.number</field>
            <field name="prefix">AT</field>
            <field name="padding">6</field>
            <field name="company_id" eval="False"/>
        </record>


        <record id="seq_asset_move" model="ir.sequence">
            <field name="name">Asset Move</field>
            <field name="code">move.number</field>
            <field name="prefix">MV</field>
            <field name="padding">6</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>
