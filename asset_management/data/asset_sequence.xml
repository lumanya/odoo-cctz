<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="seq_loan_request" model="ir.sequence">
            <field name="name">Asset Number</field>
            <field name="code">asset.number</field>
            <field name="prefix">AT</field>
            <field name="padding">6</field>
            <field name="company_id" eval="False"/>
        </record>


        <record id="seq_asset_move" model="ir.sequence">
            <field name="name">Asset Move Sequence</field>
            <field name="code">asset.move.number</field>
            <field name="prefix">MV</field>
            <field name="padding">6</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_asset_serial_number" model="ir.sequence">
            <field name="name">Asset Serial Number</field>
            <field name="code">asset.serial.number</field>
            <field name="padding">6</field>
            <field name="prefix">CC/AT/</field>
            <field name="implementation">no_gap</field>
        </record>
    </data>
</odoo>
