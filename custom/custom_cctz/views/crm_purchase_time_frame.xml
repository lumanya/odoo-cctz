<?xml version='1.0' encoding='utf-8'?>
<odoo>
    <record id= "crm_payment_time_frame_action" model="ir.actions.act_window">
        <field name="name">Purchase Time Frame</field>
        <field name="res_model">purchase.time</field>
        <field name="view_mode">tree,form</field>
    </record>

    <record id="crm_payment_time_frame_view_tree" model="ir.ui.view">
    <field name="name">payment.terms.view.tree</field>
    <field name="model">purchase.time</field>
    <field name="arch" type="xml">
        <tree string='Purchase Time Frame' editable="bottom">
            <field name="name"></field>            
        </tree>
    </field>
</record>
</odoo>