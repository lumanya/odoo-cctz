<?xml version='1.0' encoding='utf-8'?>
<odoo>
    <record id= "sector_info_action" model="ir.actions.act_window">
        <field name="name">Sectors</field>
        <field name="res_model">sector.info</field>
        <field name="view_mode">tree,form</field>
    </record>

    <record id="sector_info_view_tree" model="ir.ui.view">
    <field name="name">sector.info.view.tree</field>
    <field name="model">sector.info</field>
    <field name="arch" type="xml">
        <tree string='Sector' editable="bottom">
            <field name="name"></field>
            <field name="full_name"></field>
        </tree>
    </field>
</record>
</odoo>